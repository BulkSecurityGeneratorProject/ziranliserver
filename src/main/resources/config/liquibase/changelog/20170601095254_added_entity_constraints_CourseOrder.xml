<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity CourseOrder.
    -->
    <changeSet id="20170601095254-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="course_order"
                                 constraintName="fk_course_order_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="wechat_user"/>

        <addForeignKeyConstraint baseColumnNames="course_id"
                                 baseTableName="course_order"
                                 constraintName="fk_course_order_course_id"
                                 referencedColumnNames="id"
                                 referencedTableName="course"/>

    </changeSet>
</databaseChangeLog>
